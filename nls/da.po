#: common.c
msgid "sysstat version %s\n"
msgstr "sysstat version %s\n"

#: pidstat.c:63
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ]\n"
"Options are:\n"
"[ -C <comm> ] [ -d ] [ -I ] [ -r ] [ -u ] [ -V ]\n"
"[ -p { <pid> | SELF | ALL } ]\n"
msgstr ""
"Brug: %s [ flag... ] [ <interval> [ <repetition> ] ]\n"
"Flag:\n"
"[ -C <comm> ] [ -d ] [ -I ] [ -r ] [ -u ] [ -V ]\n"
"[ -p { <pid> | SELF | ALL } ]\n"

#: mpstat.c:62
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ]\n"
"Options are:\n"
"[ -P { <cpu> | ALL } ] [ -V ]\n"
msgstr ""
"Brug: %s [ flag... ] [ <interval> [ <repetition> ] ]\n"
"Flag:\n"
"[ -P { <cpu> | ALL } ] [ -V ]\n"

#: iostat.c:73
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ]\n"
"Options are:\n"
"[ -c ] [ -d ] [ -N ] [ -n ] [ -k | -m ] [ -t ] [ -V ] [ -x ]\n"
"[ <device> [ ... ] | ALL ] [ -p [ <device> | ALL ] ]\n"
msgstr ""
"Brug: %s [ flag... ] [ <interval> [ <repetition> ] ]\n"
"Flag:\n"
"[ -c ] [ -d ] [ -N ] [ -n ] [ -k | -m ] [ -t ] [ -V ] [ -x ]\n"
"[ <enhed> [ ... ] | ALL ] [ -p [ <enhed> | ALL ] ]\n"

#: iostat.c:555
msgid "Time: %s\n"
msgstr "Tid: %s\n"

#: iostat.c
msgid "-x and -p options are mutually exclusive\n"
msgstr "flagene -x og -p udelukker hinanden\n"

#: sar.c:91
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ]\n"
"Options are:\n"
"[ -A ] [ -b ] [ -B ] [ -c ] [ -C ] [ -d ] [ -i <interval> ] [ -p ] [ -q ]\n"
"[ -r ] [ -R ] [ -t ] [ -u ] [ -v ] [ -V ] [ -w ] [ -W ] [ -y ]\n"
"[ -I { <irq> | SUM | ALL | XALL } ] [ -P { <cpu> | ALL } ]\n"
"[ -n { DEV | EDEV | NFS | NFSD | SOCK | ALL } ]\n"
"[ -o [ <filename> ] | -f [ <filename> ] ]\n"
"[ -s [ <hh:mm:ss> ] ] [ -e [ <hh:mm:ss> ] ]\n"
msgstr ""
"Brug: %s [ flag... ] [ <interval> [ <repetition> ] ]\n"
"Flag:\n"
"[ -A ] [ -b ] [ -B ] [ -c ] [ -C ] [ -d ] [ -i <interval> ] [ -p ] [ -q ]\n"
"[ -r ] [ -R ] [ -t ] [ -u ] [ -v ] [ -V ] [ -w ] [ -W ] [ -y ]\n"
"[ -I { <irq> | SUM | ALL | XALL } ] [ -P { <cpu> | ALL } ]\n"
"[ -n { DEV | EDEV | NFS | NFSD | SOCK | ALL } ]\n"
"[ -o [ <filnavn> ] | -f [ <filnavn> ] ]\n"
"[ -s [ <tt:mm:ss> ] ] [ -e [ <tt:mm:ss> ] ]\n"

#: sar.c:184
msgid "Not an SMP machine...\n"
msgstr "Ikke et system med flere cpuer...\n"

#: sar.c:194
msgid "Not that many processors!\n"
msgstr "Der er ikke så mange cpuer!\n"

#: sar.c:284
msgid "Error while reading system activity file: %s\n"
msgstr "Læsefejl ved indlæsning af aktivitetsfil: %s\n"

#: sar.c:288
msgid "End of system activity file unexpected\n"
msgstr "Uventet afslutning på systemaktivitetsfil\n"

#: sar.c:402
msgid "Cannot open %s: %s\n"
msgstr "Kan ikke åbne %s: %s\n"

#: sar.c:521
msgid "Invalid system activity file: %s (%#x)\n"
msgstr "Ugyldig systemaktivitetsfil: %s (%#x)\n"

#:
msgid "Invalid data format\n"
msgstr "Ugyldig dataformat\n"

#: sar.c:
msgid "Average:"
msgstr "Middel:"

#: sar.c:568
msgid "Requested activities not available in file\n"
msgstr "Angivne aktiviteter findes ikke i filen\n"

#: sar.c:797
msgid "Cannot write system activity file header: %s\n"
msgstr "Kan ikke skrive systemaktivitetsfilhoved: %s\n"

#: sar.c:970
msgid "Cannot write data to system activity file: %s\n"
msgstr "Kan ikke skrive data til systemaktivitetsfil: %s\n"

#: sar.c:1419
msgid "Not reading from a system activity file (use -f option)\n"
msgstr "Læsning sker ikke fra en systemaktivitetsfil (brug flaget -f)\n"

#: sar.c:1610
msgid "-f and -o options are mutually exclusive\n"
msgstr "flagene -f og -o udelukker hinanden\n"

#: sar.c:1846
msgid "Requested activities not available\n"
msgstr "Angivne aktiviteter er ikke tilgængelig\n"

#: sadc.c:74
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ] [ <outfile> ]\n"
"Options are:\n"
"[ -C <comment> ] [ -d ] [ -F ] [ -I ] [ -V ]\n"
msgstr ""
"Brug: %s [ flag... ] [ <interval> [ <repetition> ] ] [ <resultatfil> ]\n"
"Flag:\n"
"[ -C <comment> ] [ -d ] [ -F ] [ -I ] [ -V ]\n"

#: sadc.c:614
msgid "Cannot append data to that file\n"
msgstr "Kan ikke tilføje data til den fil\n"

#: common.c:91
msgid "Cannot handle so many processors!\n"
msgstr "Kan ikke håndtere så mange cpuer!\n"

#: sadf.c:75
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ] [ <datafile> ]\n"
"Options are:\n"
"[ -d | -D | -H | -p | -x ] [ -t ] [ -V ]\n"
"[ -P { <cpu> | ALL } ] [ -s [ <hh:mm:ss> ] ] [ -e [ <hh:mm:ss> ] ]\n"
"[ -- <sar_options...> ]\n"
msgstr ""
"Brug: %s [ flag... ] [ <interval> [ <repetition> ] ] [ <datafil> ]\n"
"Flag:\n"
"[ -d | -D | -H | -p | -x ] [ -t ] [ -V ]\n"
"[ -P { <cpu> | ALL } ] [ -s [ <tt:mm:ss> ] ] [ -e [ <tt:mm:ss> ] ]\n"
"[ -- <sar_flag...> ]\n"
