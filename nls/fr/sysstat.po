#: sar.c:91
msgid "sysstat version %s\n"
"(C) S. Godard <sebastien.godard@wanadoo.fr>\n"
"Usage: %s [ options... ]\n"
"Options are:\n"
"[ -A ] [ -b ] [ -B ] [ -c ] [ -h ] [ -i <interval> ]\n"
"[ -r ] [ -R ] [ -t ] [ -u ] [ -v ] [ -V ] [ -w ] [ -W ] [ -y ]\n"
"[ -I { <irq> | SUM | PROC | ALL | XALL } ] [ -U { <cpu> | ALL } ]\n"
"[ -n { DEV | EDEV | SOCK | FULL } ]\n"
"[ -x { <pid> | SELF | SUM | ALL } ] [ -X { <pid> | SELF | ALL } ]\n"
"[ -o [ <filename> ] | -f [ <filename> ] ]\n"
"[ -s [ <hh:mm:ss> ] ] [ -e [ <hh:mm:ss> ] ]\n"
"[ <interval> [ <count> ] ]\n"
msgstr "sysstat version %s\n"
"(C) S. Godard <sebastien.godard@wanadoo.fr>\n"
"Utilisation: %s [ options... ]\n"
"Options possibles:\n"
"[ -A ] [ -b ] [ -B ] [ -c ] [ -h ] [ -i <intervalle> ]\n"
"[ -r ] [ -R ] [ -t ] [ -u ] [ -v ] [ -V ] [ -w ] [ -W ] [ -y ]\n" 
"[ -I { <irq> | SUM | PROC | ALL | XALL } ] [ -U { <cpu> | ALL } ]\n" 
"[ -n { DEV | EDEV | SOCK | FULL } ]\n" 
"[ -x { <pid> | SELF | SUM | ALL } ] [ -X { <pid> | SELF | ALL } ]\n"
"[ -o [ <fichier> ] | -f [ <fichier> ] ]\n"
"[ -s [ <hh:mm:ss> ] ] [ -e [ <hh:mm:ss> ] ]\n"
"[ <intervalle> [ <nombre_de_fois> ] ]\n"

