#: sar.c:91
msgid "sysstat version %s\n"
"(C) S. Godard <sebastien.godard@wanadoo.fr>\n"
"Usage: %s [ options... ]\n"
"Options are:\n"
"[ -A ] [ -b ] [ -B ] [ -c ] [ -d ] [ -H ] [ -h ] [ -i <interval> ] [ -q ]\n"
"[ -r ] [ -R ] [ -t ] [ -u ] [ -v ] [ -V ] [ -w ] [ -W ] [ -y ]\n"
"[ -I { <irq> | SUM | ALL | XALL } ] [ -P { <cpu> | ALL } ]\n"
"[ -n { DEV | EDEV | SOCK | FULL } ]\n"
"[ -x { <pid> | SELF | SUM | ALL } ] [ -X { <pid> | SELF | ALL } ]\n"
"[ -o [ <filename> ] | -f [ <filename> ] ]\n"
"[ -s [ <hh:mm:ss> ] ] [ -e [ <hh:mm:ss> ] ]\n"
"[ <interval> [ <count> ] ]\n"
msgstr "sysstat versione %s\n"
"(C) S. Godard <sebastien.godard@wanadoo.fr>\n"
"Uso: %s [ opzioni... ]\n"
"Opzioni ammesse:\n"
"[ -A ] [ -b ] [ -B ] [ -c ] [ -d ] [ -H ] [ -h ] [ -i <intervallo> ] [ -q ]\n"
"[ -r ] [ -R ] [ -t ] [ -u ] [ -v ] [ -V ] [ -w ] [ -W ] [ -y ]\n"
"[ -I { <irq> | SUM | ALL | XALL } ] [ -P { <cpu> | ALL } ]\n"
"[ -n { DEV | EDEV | SOCK | FULL } ]\n"
"[ -x { <pid> | SELF | SUM | ALL } ] [ -X { <pid> | SELF | ALL } ]\n"
"[ -o [ <file> ] | -f [ <file> ] ]\n"
"[ -s [ <hh:mm:ss> ] ] [ -e [ <hh:mm:ss> ] ]\n"
"[ <intervallo> [ <iterazioni> ] ]\n"

