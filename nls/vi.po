#, c-format
msgid "Size of a long int: %d\n"
msgstr "Kích cỡ của một số nguyên dài: %d\n"

#: sadf.c:1782
#, c-format
msgid "Number of CPU: %u\n"
msgstr "Số CPU: %u\n"

#: sadf.c:1783
#, c-format
msgid "Number of interrupts per CPU: %u\n"
msgstr "Số lần gián đoạn trong mỗi CPU: %u\n"

#: sadf.c:1784
#, c-format
msgid "Number of disks: %u\n"
msgstr "Số đĩa: %u\n"

#: sadf.c:1785
#, c-format
msgid "Number of serial lines: %u\n"
msgstr "Số đường nối tiếp: %u\n"

#: sadf.c:1786
