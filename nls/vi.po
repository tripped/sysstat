#: common.c:57
#, c-format
msgid "sysstat version %s\n"
msgstr "sysstat phiên bản %s\n"

#: common.c:160 common.c:385 ioconf.c:459 mpstat.c:265 sa_common.c:601
#: sadc.c:476 sadc.c:485 sadc.c:530 sadc.c:620
#, c-format
msgid "Cannot open %s: %s\n"
msgstr "Không thể mở %s: %s\n"

#: common.c:199
#, c-format
msgid "Cannot handle so many processors!\n"
msgstr "Không thể quản lý quá nhiều bộ xử lý !\n"

#: iostat.c:74
#, c-format
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ]\n"
"Options are:\n"
"[ -c ] [ -d ] [ -N ] [ -n ] [ -k | -m ] [ -t ] [ -V ] [ -x ]\n"
"[ <device> [ ... ] | ALL ] [ -p [ <device> | ALL ] ]\n"
msgstr ""
"Sử dụng: %s [ tùy_chọn... ] [ <khoảng> [ <đếm> ] ]\n"
"\n"
"Tùy chọn:\n"
"[ -c ] [ -d ] [ -N ] [ -n ] [ -k | -m ] [ -t ] [ -V ] [ -x ]\n"
"[ <thiết_bị> [ ... ] | ALL ] [ -p [ <thiết_bị> | ALL ] ]\n"
"\n"
"ALL\ttất cả\n"
"\n"

#: iostat.c:1119
#, c-format
msgid "Time: %s\n"
msgstr "Thời gian: %s\n"

#: iostat.c:1435
#, c-format
msgid "-x and -p options are mutually exclusive\n"
msgstr "Hai tùy chọn « -x » và « -p » loại từ lẫn nhau\n"

#: mpstat.c:66
#, c-format
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ]\n"
"Options are:\n"
"[ -P { <cpu> | ALL } ] [ -V ]\n"
msgstr ""
"Sử dụng: %s [ tùy_chọn... ] [ <khoảng> [ <đếm> ] ]\n"
"\n"
"Tùy chọn:\n"
"[ -P { <cpu> | ALL } ] [ -V ]\n"
"\n"
"ALL\ttất cả\n"
"\n"

#: mpstat.c:226 pidstat.c:1148 sar.c:740
msgid "Average:"
msgstr "Trung bình:"

#: mpstat.c:563 sar.c:157
#, c-format
msgid "Not that many processors!\n"
msgstr "Quá nhiều bộ xử lý !\n"

#: pidstat.c:72
#, c-format
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ]\n"
"Options are:\n"
"[ -C <comm> ] [ -d ] [ -I ] [ -r ] [ -t ] [ -u ] [ -V ] [ -w ]\n"
"[ -p { <pid> | SELF | ALL } ] [ -T { TASK | CHILD | ALL } ]\n"
msgstr ""
"Sử dụng: %s [ tùy_chọn... ] [ <khoảng> [ <đếm> ] ]\n"
"\n"
"Tùy chọn:\n"
"[ -C <comm> ] [ -d ] [ -I ] [ -r ] [ -t ] [ -u ] [ -V ] [ -w ]\n"
"[ -p { <pid> | SELF | ALL } ] [ -T { TASK | CHILD | ALL } ]\n"
"\n"
"SELF\t\tchính nó\n"
"ALL\t\ttất cả\n"
"TASK\ttác vụ\n"
"CHILD\tđiều con\n"
"\n"

#: pidstat.c:166 sar.c:1419
#, c-format
msgid "Requested activities not available\n"
msgstr "Hoạt động đã yêu cầu vẫn không sẵn sàng\n"

#: sa_common.c:572
#, c-format
msgid "Error while reading system activity file: %s\n"
msgstr "Gặp lỗi khi đọc tập tin hoạt động hệ thống: %s\n"

#: sa_common.c:581
#, c-format
msgid "End of system activity file unexpected\n"
msgstr "Gặp kết thúc bất thường trong tập tin hoạt động hệ thống\n"

#: sa_common.c:608 sadc.c:549
#, c-format
msgid "Invalid system activity file: %s (%#x)\n"
msgstr "Tập tin hoạt động hệ thống không hợp lệ: %s (%#x)\n"

#: sa_common.c:632
#, c-format
msgid "Requested activities not available in file\n"
msgstr "Hoạt động đã yêu cầu vẫn không sẵn sàng trong tập tin\n"

#: sadc.c:83
#, c-format
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ] [ <outfile> ]\n"
"Options are:\n"
"[ -C <comment> ] [ -d ] [ -F ] [ -I ] [ -V ]\n"
msgstr ""
"Sử dụng: %s [ tùy_chọn... ] [ <khoảng> [ <đếm> ] ] [ <tập_tin_xuất> ]\n"
"\n"
"Tùy chọn:\n"
"[ -C <chú_thích> ] [ -d ] [ -F ] [ -I ] [ -V ]\n"

#: sadc.c:110
#, c-format
msgid "Cannot write data to system activity file: %s\n"
msgstr "Không thể ghi dữ liệu vào tập tin hoạt động hệ thống: %s\n"

#: sadc.c:361
#, c-format
msgid "Cannot write system activity file header: %s\n"
msgstr "Không thể ghi phần đầu tập tin hoạt động hệ thống: %s\n"

#: sadc.c:568
#, c-format
msgid "Cannot append data to that file\n"
msgstr "Không thể phụ thêm dữ liệu vào tập tin đó\n"

#: sadf.c:78
#, c-format
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ] [ <datafile> ]\n"
"Options are:\n"
"[ -d | -D | -H | -p | -x ] [ -t ] [ -V ]\n"
"[ -P { <cpu> | ALL } ] [ -s [ <hh:mm:ss> ] ] [ -e [ <hh:mm:ss> ] ]\n"
"[ -- <sar_options...> ]\n"
msgstr ""
"Sử dụng: %s [ tùy_chọn... ] [ <khoảng> [ <đếm> ] ] [ <tập_tin_dữ_liệu> ]\n"
"\n"
"Tùy chọn:\n"
"[ -d | -D | -H | -p | -x ] [ -t ] [ -V ]\n"
"[ -P { <cpu> | ALL } ] [ -s [ <hh:mm:ss> ] ] [ -e [ <hh:mm:ss> ] ]\n"
"[ -- <tùy_chọn_sar...> ]\n"
"\n"
"ALL\t\t\ttất cảhh:mm:ss\tthời gian có dạng « giờ:phút:giây » (mỗi phần có hai chữ số)\n"
"\n"

#: sar.c:76
#, c-format
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ]\n"
"Options are:\n"
"[ -A ] [ -b ] [ -B ] [ -c ] [ -C ] [ -d ] [ -i <interval> ] [ -p ] [ -q ]\n"
"[ -r ] [ -R ] [ -t ] [ -u ] [ -v ] [ -V ] [ -w ] [ -W ] [ -y ]\n"
"[ -I { <irq> | SUM | ALL | XALL } ] [ -P { <cpu> | ALL } ]\n"
"[ -n { DEV | EDEV | NFS | NFSD | SOCK | ALL } ]\n"
"[ -o [ <filename> ] | -f [ <filename> ] ]\n"
"[ -s [ <hh:mm:ss> ] ] [ -e [ <hh:mm:ss> ] ]\n"
msgstr ""
"Sử dụng: %s [ tùy_chọn... ] [ <khoảng> [ <đếm> ] ]\n"
"\n"
"Tùy chọn:\n"
"[ -A ] [ -b ] [ -B ] [ -c ] [ -C ] [ -d ] [ -i <khoảng> ] [ -p ] [ -q ]\n"
"[ -r ] [ -R ] [ -t ] [ -u ] [ -v ] [ -V ] [ -w ] [ -W ] [ -y ]\n"
"[ -I { <irq> | SUM | ALL | XALL } ] [ -P { <cpu> | ALL } ]\n"
"[ -n { DEV | EDEV | NFS | NFSD | SOCK | ALL } ]\n"
"[ -o [ <tên_tập_tin> ] | -f [ <tên_tập_tin> ] ]\n"
"[ -s [ <hh:mm:ss> ] ] [ -e [ <hh:mm:ss> ] ]\n"
"\n"
"SUM\t\ttổng\n"
"ALL\t\ttất cả\n"
"XALL\tkhông phải tất cả\n"
"DEV\t\tthiết bị\n"
"SOCK\tổ cắm\n"
"hh:mm:ss\tthời gian có dạng « giờ:phút:giây » (mỗi phần có hai chữ số)\n"
"\n"

#: sar.c:149
#, c-format
msgid "Not an SMP machine...\n"
msgstr "Không phải máy SMP...\n"

#: sar.c:1261
#, c-format
msgid "Invalid data format\n"
msgstr "Định dạng dữ liệu không hợp lệ\n"

#: sar.c:1633
#, c-format
msgid "-f and -o options are mutually exclusive\n"
msgstr "Hai tùy chọn « -f » và « -o » loại từ lẫn nhau\n"

#: sar.c:1639
#, c-format
msgid "Not reading from a system activity file (use -f option)\n"
msgstr "Không phải đọc từ tập tin hoạt động hệ thống (dùng tùy chọn « -f »)\n"
